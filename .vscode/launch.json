{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug API",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/api",
      "cwd": "${workspaceFolder}/api",
      "env": {
        "CLIENT_ID": "datamonster",
        "CLIENT_SECRET": "dev-local-secret",
        "ISSUER_URL": "https://127.0.0.1:9091",
        "REDIRECT_URL": "http://localhost:3000/auth/callback",
        "INTROSPECT_URL": "https://127.0.0.1:9091/api/oidc/introspection",
        "TOKEN_URL":"https://127.0.0.1:9091/api/oidc/tokenn",
        "CLIENT_URL": "http://localhost:3000/",
        "DBSN": "postgres://datamonster:datamonster@localhost:5432/datamonster?sslmode=disable",
        "COOKIE_SECURE": "false",
        "VALKEY_ADDR": "localhost:6379"
      }
    }
  ]
}
